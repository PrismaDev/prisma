<script type="text/underscore-template" id="suggestions-template">
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
		<h3><%= str.get('dialogTitle') %></h3>
	</div>
	<form id="suggestions-form" action="/api/sugestao" method="post">
		<div class="modal-body">
			<textarea name="comentario" class="input-block-level"></textarea>
		</div>
		<div class="modal-footer">
			<input type="reset" class="btn btn-danger" value="<%= str.get('resetButtonLabel') %>">
			<input type="submit" class="btn btn-primary" value="<%= str.get('submitButtonLabel') %>">
		</div>
	</form>
</script>
