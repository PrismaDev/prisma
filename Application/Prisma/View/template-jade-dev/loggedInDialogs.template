<script id="suggestions-template" type="text/underscore-template"><div class="modal-header"><button type="button" data-dismiss="modal" aria-hidden="true" class="close">&times;</button><h3><%= suggestionsStr.get("dialogTitle") %></h3></div><form id="suggestions-form" action="/api/sugestao" method="post"><div class="modal-body"><textarea name="comentario" class="input-block-level"></textarea></div><div class="modal-footer"><input type="reset" value="<%=suggestionsStr.get('resetButtonLabel') %>" class="btn btn-danger"/><input type="submit" value="<%=suggestionsStr.get('submitButtonLabel') %>" class="btn btn-primary disabled"/></div></form></script><script id="updates-template" type="text/underscore-template"><div class="modal-header"><button type="button" data-dismiss="modal" aria-hidden="true" class="close">&times;</button><h3 id="updates-title"><%= updatesStr.get('dialogTitle') %></h3><a href="https://twitter.com/<%= layoutStr.get('twitter') %>" data-show-count="false" data-lang="en" data-show-screen-name="false" class="twitter-follow-button">Follow @<%= layoutStr.get('twitter') %></a></div><div class="modal-body"><a href="https://twitter.com/<%= layoutStr.get('twitter') %>" data-widget-id="<%= layoutStr.get('twitterId') %>" height="350" data-chrome="noheader" class="twitter-timeline">Tweets by @<%= layoutStr.get('twitter') %></a></div></script>