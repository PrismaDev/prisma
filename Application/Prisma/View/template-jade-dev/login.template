<script id="login-template" type="text/underscore-template"><% if (error && error!='invalidLogin') { %><div class="row-fluid"><div class="span6 offset3"><div class="alert alert-error centered"><%= error=='accessDenied'? loginStr.get('accessDeniedErrorMsg') : '' %>
<%= error=='pageDoesNotExist'? loginStr.get('pageDoesNotExistErrorMsg') : '' %>
<%= error=='unexpectedError'? loginStr.get('unexpectedErrorMsg') : '' %></div></div></div><% } %><div class="row-fluid"><div id="logo-div" class="span4 offset4"><h1><%= layoutStr.get('projectNameStr') %></h1></div></div><div id="login-container"><div class="row-fluid"><div class="span4 offset4 content"><%= loginStr.get('infoText') %>	</div></div></div></script>