<script id="microhorario-template" type="text/underscore-template"><div id="microhorario-togglefilter"><input id="open-filters-button" type="button" value="<%= str.get('openFiltersStr') %>" class="btn btn-link hidden pull-right"/><input id="close-filters-button" type="button" value="<%= str.get('closeFiltersStr') %>" class="btn btn-link pull-right"/></div><div id="microhorario-filter"><form id="microhorario-form"><div class="field-container"><label for="CodigoDisciplina"><%= str.get("subjectCodeLabel") %></label><input type="text" name="CodigoDisciplina"/></div><div class="field-container"><label for="NomeDisciplina"><%= str.get("subjectNameLabel") %></label><input type="text" name="NomeDisciplina"/></div><div class="field-container"><label for="NomeProfessor"><%= str.get("professorNameLabel") %></label><input type="text" name="NomeProfessor"/></div><div class="field-container"><label for="DiaSemana"><%= str.get('dayLabel') %></label><select name="DiaSemana"><option value=""></option><% _.each(timetableStr.get('daysLabel'), function(day, index) { %><option value="<%= index+2 %>"><%= day %></option><% }); %></select></div><div id="hidden-filters" class="hidden"><div class="field-container"><label for="Creditos"><%= str.get("nCreditsLabel") %></label><input type="text" name="Creditos" class="input-small"/></div><div class="field-container"><label for="HoraInicial"><%= str.get("timeBeginLabel") %></label><input type="text" name="HoraInicial" class="input-small"/></div><div class="field-container"><label for="HoraFinal"><%= str.get("timeEndLabel") %></label><input type="text" name="HoraFinal" class="input-small"/></div><div class="checkboxes-div"><label for="SHF" class="checkbox inline"><input type="checkbox" name="SHF" value="1"/><%= str.get("noFixedScheduleLabel") %></label><label for="HorasDistancia" class="checkbox inline"><input type="checkbox" name="HorasDistancia" value="1"/><%= str.get("distanceHoursLabel") %></label></div></div><div class="button-container"><input id="less-filters-button" type="button" value="<%= str.get('lessFiltersStr') %>" class="btn btn-link hidden"/><input id="more-filters-button" type="button" value="<%= str.get('moreFiltersStr') %>" class="btn btn-link"/><input id="submit-query" type="submit" value="<%= str.get('submitButtonLabel') %>" class="btn btn-primary pull-right"/><input id="reset-query" type="reset" value="<%= str.get('resetButtonLabel') %>" class="btn btn-danger pull-right"/></div></form></div><div id="microhorario-results"></div></script><script id="microhorario-waiting-template" type="text/underscore-template"><img src="<%= str.get('waitingImgURL') %>" class="waiting-img"/></script><script id="microhorario-noquery-template" type="text/underscore-template"><div class="alert alert-info"><%= str.get('noQueryStr') %></div></script><script id="microhorario-end-of-data-template" type="text/underscore-template"><div class="alert"><%= str.get('endOfDataMsg') %></div></script>